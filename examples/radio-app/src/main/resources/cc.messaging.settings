[messaging]
discovery-directories-domain=io.joynr

capabilities-directory-channelid=discoverydirectory_channelid
capabilities-directory-participantid=capabilitiesdirectory_participantid

mqtt-keep-alive-time-seconds=60
mqtt-reconnect-sleep-time-ms=1000

send-msg-retry-interval=500
persistence-file=joynr.settings

[lib-joynr]
participant-ids-persistence-file=cc.ParticipantIds.persist
local-capabilities-directory-persistence-file=cc.LocalCapabilitiesDirectory.persist

[uds]
socket-path=/tmp/joynr-radio-app.sock
